<?xml version="1.0" encoding="UTF-8"?>


<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <property  name="now" dbms="mysql" value="now()"></property>
    <property  name="now" dbms="oracle" value="sysdate"></property>
    <property  name="now" dbms="db2" value="current_timestamp"></property>
    <changeSet id="imageVideo-dictionary" author="yangqiu" runOnChange="true">
        <preConditions onFail="CONTINUE">
            <and>
                <sqlCheck expectedResult="0">select count(*) from yd_sys_dictionary where yd_id = 42119222946722</sqlCheck>
                <not>
                    <sqlCheck expectedResult="0">select count(*) from yd_sys_dictionary</sqlCheck>
                </not>
               <!-- <sqlCheck expectedResult="0">select count(*) from yd_sys_option where yd_dictionary_id = 32119222946719</sqlCheck>-->
            </and>

        </preConditions>
        <insert tableName="yd_sys_dictionary">
            <column name="yd_id" value="42119222946722"></column>
            <column name="yd_created_by" value="2"/>
            <column name="yd_created_date" valueDate="${now}"/>
            <column name="yd_last_update_by" value="2"/>
            <column name="yd_last_update_date" valueDate="${now}"/>
            <column name="yd_version_ct" value="0"/>
            <column name="yd_deleted" valueBoolean="false"/>
            <column name="yd_name" value="imageVideoErrorType"/>
        </insert>
    </changeSet>
    <changeSet id="imageVideo-dictionary_option" author="yangqiu" runOnChange="true">
        <preConditions onFail="CONTINUE">
            <and>
                <sqlCheck expectedResult="0">select count(*) from yd_sys_option where yd_dictionary_id = 42119222946722</sqlCheck>
                <not>
                    <sqlCheck expectedResult="0">select count(*) from yd_sys_option</sqlCheck>
                </not>
            </and>
        </preConditions>
        <insert tableName="yd_sys_option">
            <column name="yd_id" valueComputed="(SELECT MAX(ysp1.yd_id)+1 FROM yd_sys_option ysp1)"></column>
            <column name="yd_created_by" value="2"/>
            <column name="yd_created_date" valueDate="${now}"/>
            <column name="yd_last_update_by" value="2"/>
            <column name="yd_last_update_date" valueDate="${now}"/>
            <column name="yd_version_ct" value="0"/>
            <column name="yd_dictionary_id" value="42119222946722"/>
            <column name="yd_name" value="双录视频缺失"/>
