<?xml version="1.0" encoding="UTF-8"?>


<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <property  name="now" dbms="mysql" value="now()"></property>
    <property  name="now" dbms="oracle" value="sysdate"></property>
    <property  name="now" dbms="db2" value="current_timestamp"></property>
    <changeSet id="jiebenChange-cancel-01" author="yangqiu" runOnChange="true">
        <preConditions onFail="CONTINUE">
            <and>
                <sqlCheck expectedResult="0">select count(*) from yd_sys_account_configure where yd_acct_type = 'jiben' and yd_depositor_type='01' and yd_operate_type='ACCT_CHANGE'</sqlCheck>
                <not>
                    <sqlCheck expectedResult="0">select count(*) from yd_sys_account_configure</sqlCheck>
                </not>
            </and>
        </preConditions>
        <insert tableName="yd_sys_account_configure">
            <column name="yd_id" valueComputed="(SELECT MAX(ysp1.yd_id)+1 FROM yd_sys_account_configure ysp1)"></column>
            <column name="yd_created_by" value="2"/>
            <column name="yd_created_date" valueDate="${now}"/>
            <column name="yd_last_update_by" value="2"/>
            <column name="yd_last_update_date" valueDate="${now}"/>
            <column name="yd_version_ct" value="0"/>
            <column name="yd_acct_type" value="jiben"/>
            <column name="yd_depositor_type" value="01"/>
            <column name="yd_operate_type" value="ACCT_CHANGE"/>
        </insert>
    </changeSet>

