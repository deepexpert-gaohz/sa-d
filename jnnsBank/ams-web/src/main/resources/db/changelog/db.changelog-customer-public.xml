<?xml version="1.0" encoding="UTF-8"?>


<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="customer_public_setorganfullid" author="taire" runOnChange="true">
        <preConditions onFail="CONTINUE">
            <and>
                <not>
                    <sqlCheck expectedResult="0">SELECT count(*) from yd_customer_public ap</sqlCheck>
                </not>
                <sqlCheck expectedResult="0">SELECT count(*) from yd_customer_public ap where yd_organ_full_id is not NULL</sqlCheck>
                <sqlCheck expectedResult="0">SELECT count(*) from yd_customer_public ap where yd_depositor_name is not NULL</sqlCheck>
            </and>
        </preConditions>
        <sqlFile encoding="utf8"
                 endDelimiter=";"
                 path="sql/schema-customerpublic-updateField.sql"
                 relativeToChangelogFile="true"
                 splitStatements="true"
                 stripComments="true"/>
    </changeSet>

</databaseChangeLog>