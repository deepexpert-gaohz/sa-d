    <changeSet id="2" author="fantao" runOnChange="true">
        <preConditions onFail="CONTINUE">
            <not>
                <tableExists tableName="SPRING_SESSION"/>
            </not>
        </preConditions>
        <sqlFile dbms="mysql"
                 encoding="utf8"
                 endDelimiter=";"
                 path="sql/schema-mysql.sql"
                 relativeToChangelogFile="true"
                 splitStatements="true"
                 stripComments="true"/>
    </changeSet>
    <changeSet id="3" author="fantao" runOnChange="true">
        <preConditions onFail="CONTINUE">
            <not>
                <tableExists tableName="SPRING_SESSION"/>
            </not>
        </preConditions>
        <sqlFile dbms="oracle"
                 encoding="utf8"
                 endDelimiter=";"
                 path="sql/schema-oracle.sql"
                 relativeToChangelogFile="true"
                 splitStatements="true"
                 stripComments="true"/>
    </changeSet>
    <changeSet id="4" author="fantao" runOnChange="true">
        <preConditions onFail="CONTINUE">
            <not>
                <tableExists tableName="SPRING_SESSION"/>
            </not>
        </preConditions>
        <sqlFile dbms="db2"
                 encoding="utf8"
                 endDelimiter=";"
                 path="sql/schema-db2.sql"
                 relativeToChangelogFile="true"
                 splitStatements="true"
                 stripComments="true"/>
    </changeSet>
</databaseChangeLog>