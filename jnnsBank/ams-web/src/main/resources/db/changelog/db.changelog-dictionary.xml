    <changeSet id="yd_dictionary" author="yangqiu" runOnChange="true">
        <preConditions onFail="CONTINUE">
            <and>
                <sqlCheck expectedResult="0">select count(*) from yd_sys_dictionary where yd_id = 32119222946719</sqlCheck>
                <not>
                    <sqlCheck expectedResult="0">select count(*) from yd_sys_dictionary</sqlCheck>
                </not>
               <!-- <sqlCheck expectedResult="0">select count(*) from yd_sys_option where yd_dictionary_id = 32119222946719</sqlCheck>-->
            </and>

        </preConditions>
        <insert tableName="yd_sys_dictionary">
            <column name="yd_id" value="32119222946719"></column>
            <column name="yd_created_by" value="2"/>
            <column name="yd_created_date" valueDate="${now}"/>
            <column name="yd_last_update_by" value="2"/>
            <column name="yd_last_update_date" valueDate="${now}"/>
            <column name="yd_version_ct" value="0"/>
            <column name="yd_deleted" valueBoolean="false"/>
            <column name="yd_name" value="idCardedType"/>
        </insert>
    </changeSet>
