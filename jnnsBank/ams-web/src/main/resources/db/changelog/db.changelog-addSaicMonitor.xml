<?xml version="1.0" encoding="UTF-8"?>


<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <property  name="now" dbms="mysql" value="now()"></property>
    <property  name="now" dbms="oracle" value="sysdate"></property>
    <property  name="now" dbms="db2" value="current_timestamp"></property>
    <changeSet id="permission-addSaicMonitor" author="chenluxiang" runOnChange="true">
        <preConditions onFail="CONTINUE">
            <and>
                <sqlCheck expectedResult="1">select count(*) from yd_sys_permission where yd_code = 'sys_manager'</sqlCheck>
                <sqlCheck expectedResult="0">select count(*) from yd_sys_permission where yd_code = 'system_interface_monitor'</sqlCheck>
            </and>
        </preConditions>
        <insert tableName="yd_sys_permission">
            <column name="yd_id" value="1090115123548165"></column>
            <column name="yd_created_by" value="2"/>
            <column name="yd_created_date" valueDate="${now}"/>
            <column name="yd_last_update_by" value="2"/>
            <column name="yd_last_update_date" valueDate="${now}"/>
            <column name="yd_version_ct" value="0"/>
            <column name="yd_code" value="system_interface_monitor"/>
            <column name="yd_deleted" valueBoolean="false"/>
            <column name="yd_description" value="系统接口检测"/>
            <column name="yd_enabled" valueBoolean="true"/>
            <column name="yd_icon" value="fa-hand-rock-o"/>
            <column name="yd_method" value="GET"/>
            <column name="yd_order_num" value="0"/>
            <column name="yd_parent_id" valueComputed="-1"/>
            <column name="yd_permission_type" value="menu"/>
            <column name="yd_title" value="系统接口检测"/>
            <column name="yd_url" value=""/>
        </insert>
    </changeSet>

    <changeSet id="permission-addsystem_saic_monitor" author="chenluxiang" runOnChange="true">
        <preConditions onFail="CONTINUE">
            <and>
                <sqlCheck expectedResult="1">select count(*) from yd_sys_permission where yd_code = 'system_interface_monitor'</sqlCheck>
                <sqlCheck expectedResult="0">select count(*) from yd_sys_permission where yd_code = 'system_saic_monitor'</sqlCheck>
            </and>
        </preConditions>
        <insert tableName="yd_sys_permission">
            <column name="yd_id" valueComputed="(SELECT MAX(ysp1.yd_id)+1 FROM yd_sys_permission ysp1)"></column>
            <column name="yd_created_by" value="2"/>
            <column name="yd_created_date" valueDate="${now}"/>
            <column name="yd_last_update_by" value="2"/>
            <column name="yd_last_update_date" valueDate="${now}"/>
            <column name="yd_version_ct" value="0"/>
            <column name="yd_code" value="system_saic_monitor"/>
            <column name="yd_deleted" valueBoolean="false"/>
            <column name="yd_description" value="工商接口监测"/>
            <column name="yd_enabled" valueBoolean="true"/>
            <column name="yd_icon" value="fa-hand-rock-o"/>
            <column name="yd_method" value="GET"/>
            <column name="yd_order_num" value="0"/>
            <column name="yd_parent_id" value="1090115123548165"/>
            <column name="yd_permission_type" value="menu"/>
            <column name="yd_title" value="工商接口监测"/>
            <column name="yd_url" value="../ui/saicInterfaceMonitor/list.html"/>
        </insert>
    </changeSet>


</databaseChangeLog>