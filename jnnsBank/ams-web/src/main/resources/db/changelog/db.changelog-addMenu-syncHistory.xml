<?xml version="1.0" encoding="UTF-8"?>


<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <property  name="now" dbms="mysql" value="now()"></property>
    <property  name="now" dbms="oracle" value="sysdate"></property>
    <property  name="now" dbms="db2" value="current_timestamp"></property>
    <changeSet id="sync_history" author="yangqiu" runOnChange="true">
        <preConditions onFail="CONTINUE">
            <and>
                <sqlCheck expectedResult="0">select count(*) from yd_sys_permission where yd_code = 'sync_history'</sqlCheck>
                <not>
                    <sqlCheck expectedResult="0">select count(*) from yd_sys_permission</sqlCheck>
                </not>
            </and>
        </preConditions>
        <insert tableName="yd_sys_permission">
            <column name="yd_id" valueComputed="(SELECT MAX(ysp1.yd_id)+1 FROM yd_sys_permission ysp1)"/>
            <column name="yd_created_by" value="2"/>
            <column name="yd_created_date" valueDate="${now}"/>
            <column name="yd_last_update_by" value="2"/>
            <column name="yd_last_update_date" valueDate="${now}"/>
            <column name="yd_version_ct" value="0"/>
            <column name="yd_code" value="sync_history"/>
            <column name="yd_deleted" valueBoolean="false"/>
            <column name="yd_description" value="报送监管"/>
            <column name="yd_enabled" valueBoolean="true"/>
            <column name="yd_icon" value="fa fa-sitemap"/>
            <column name="yd_method" value="GET"/>
            <column name="yd_order_num" value="150000"/>
            <column name="yd_parent_id" value="-1"/>
            <column name="yd_permission_type" value="menu"/>
            <column name="yd_title" value="报送监管"/>
            <column name="yd_url" value=""/>
        </insert>
    </changeSet>

    <changeSet id="sync_history_ams" author="yangqiu" runOnChange="true">
        <preConditions onFail="CONTINUE">
            <and>
                <sqlCheck expectedResult="1">select count(*) from yd_sys_permission where yd_code = 'sync_history'</sqlCheck>
                <sqlCheck expectedResult="0">select count(*) from yd_sys_permission where yd_code = 'sync_history_ams'</sqlCheck>
            </and>
        </preConditions>
        <insert tableName="yd_sys_permission">
            <column name="yd_id" valueComputed="(SELECT MAX(ysp1.yd_id)+1 FROM yd_sys_permission ysp1)"/>
            <column name="yd_created_by" value="2"/>
            <column name="yd_created_date" valueDate="${now}"/>
            <column name="yd_last_update_by" value="2"/>
            <column name="yd_last_update_date" valueDate="${now}"/>
            <column name="yd_version_ct" value="0"/>
            <column name="yd_code" value="sync_history_ams"/>
            <column name="yd_deleted" valueBoolean="false"/>
            <column name="yd_description" value="人行账管报送监管"/>
            <column name="yd_enabled" valueBoolean="true"/>
            <column name="yd_icon" value="fa fa-university"/>
            <column name="yd_method" value="GET"/>
            <column name="yd_order_num" value="150001"/>
            <column name="yd_parent_id" valueComputed="(SELECT ysp2.yd_id FROM yd_sys_permission ysp2 where ysp2.yd_code = 'sync_history')"/>
            <column name="yd_permission_type" value="menu"/>
            <column name="yd_title" value="人行账管报送监管"/>
            <column name="yd_url" value="../ui/syncHistory/list.html?syncType=AMS"/>
        </insert>
    </changeSet>

