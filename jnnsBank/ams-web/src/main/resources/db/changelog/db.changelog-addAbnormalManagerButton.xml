        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <property  name="now" dbms="mysql" value="now()"/>
    <property  name="now" dbms="oracle" value="sysdate"/>
    <property  name="now" dbms="db2" value="current_timestamp"/>
    <changeSet id="permission-customerAbnormal_manager_export" author="houxianghua" runOnChange="true">
        <preConditions onFail="CONTINUE">
            <and>
                <sqlCheck expectedResult="1">select count(*) from yd_sys_permission where yd_code = 'customerAbnormal_manager'</sqlCheck>
                <sqlCheck expectedResult="0">select count(*) from yd_sys_permission where yd_code = 'customerAbnormal_manager:export'</sqlCheck>
            </and>
        </preConditions>
        <insert tableName="yd_sys_permission">
            <column name="yd_id" valueComputed="(SELECT MAX(ysp1.yd_id)+1 FROM yd_sys_permission ysp1)"/>
            <column name="yd_created_by" value="2"/>
            <column name="yd_created_date" valueDate="${now}"/>
            <column name="yd_last_update_by" value="2"/>
            <column name="yd_last_update_date" valueDate="${now}"/>
            <column name="yd_version_ct" value="0"/>
            <column name="yd_code" value="customerAbnormal_manager:export"/>
            <column name="yd_deleted" valueBoolean="false"/>
            <column name="yd_description" value="导出"/>
            <column name="yd_enabled" valueBoolean="true"/>
            <column name="yd_icon" value=""/>
            <column name="yd_method" value="GET"/>
            <column name="yd_order_num" value="175"/>
            <column name="yd_parent_id" valueComputed="(SELECT ysp2.yd_id FROM yd_sys_permission ysp2 where ysp2.yd_code = 'customerAbnormal_manager')"/>
            <column name="yd_permission_type" value="button"/>
            <column name="yd_title" value="导出"/>
            <column name="yd_url" value="../ui/customer/abnormal.html"/>
        </insert>
    </changeSet>
