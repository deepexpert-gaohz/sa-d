<?xml version="1.0" encoding="UTF-8"?>


<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <property  name="now" dbms="mysql" value="now()"></property>
    <property  name="now" dbms="oracle" value="sysdate"></property>
    <property  name="now" dbms="db2" value="current_timestamp"></property>
    <changeSet id="dictionary-industryCodeOption" author="chenluxiang" runOnChange="true">
        <preConditions onFail="CONTINUE">
            <not>
                <sqlCheck expectedResult="0">select count(*) from yd_sys_option </sqlCheck>
            </not>
        </preConditions>
        <update tableName="yd_sys_option">
            <column name="yd_name" value="F：交通运输、仓储及邮政业"/>
            <where>yd_name='F：交通运输、仓库及邮政业'</where>
        </update>
        <update tableName="yd_sys_option">
            <column name="yd_value" value="交通运输、仓储及邮政业"/>
            <where>yd_value='交通运输、仓库及邮政业'</where>
        </update>
    </changeSet>
</databaseChangeLog>