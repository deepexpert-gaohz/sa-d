<?xml version="1.0" encoding="UTF-8"?>


<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <property  name="now" dbms="mysql" value="now()"></property>
    <property  name="now" dbms="oracle" value="sysdate"></property>
    <property  name="now" dbms="db2" value="current_timestamp"></property>
    <changeSet id="customerTune_update" author="taire" runOnChange="true">
        <preConditions onFail="CONTINUE">
            <and>
                <sqlCheck expectedResult="1">select count(*) from yd_sys_permission where yd_code = 'customerManager_search'</sqlCheck>
            </and>
        </preConditions>

        <update tableName="yd_sys_permission">
            <column name="yd_icon" value="fa-credit-card"/>
            <where>yd_code = 'pbc_result_byAcctNo'</where>
        </update>
        <update tableName="yd_sys_permission">
            <column name="yd_icon" value="fa-certificate"/>
            <where>yd_code = 'pbc_result_byAccountKey'</where>
        </update>
        <update tableName="yd_sys_permission">
            <column name="yd_icon" value="fa-hand-o-right"/>
            <where>yd_code = 'eccs_result_search'</where>
        </update>
        <update tableName="yd_sys_permission">
            <column name="yd_icon" value="fa-hdd-o"/>
            <where>yd_code = 'saicInfo_byName'</where>
        </update>
        <update tableName="yd_sys_permission">
            <column name="yd_icon" value="fa-bullhorn"/>
            <where>yd_code = 'carrierOperator_search'</where>
        </update>
        <update tableName="yd_sys_permission">
            <column name="yd_icon" value="fa-bell"/>
            <where>yd_code = 'idCard_search'</where>
        </update>
    </changeSet>
</databaseChangeLog>